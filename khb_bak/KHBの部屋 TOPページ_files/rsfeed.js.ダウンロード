function loadFeed() {
    // 初期化
    var feed = new google.feeds.Feed("http://members.redsonline.jp/rss/redstone_rss.asp");
    feed.setNumEntries(100);  // 最大記事100件まで
    // 記事を読み込む
    feed.load(function(result) {
        var html;
        // 読み込みに成功したときの処理
        if (!result.error) {
            // サイトのタイトルを出力
            html = '<B>＜<a href="' + result.feed.link + '" target="_blank">' + 'RS公式 新着情報' + '</a>＞</B>';
            // 各記事の情報を順に出力
            if (result.feed.entries.length) {
                // 記事を日付降順でソート
                articlesort = new Array(result.feed.entries.length);
                articlesort[0] = 0;
                for ( i = 1 ; i < result.feed.entries.length; i++ ){
                  targetdate = new Date(result.feed.entries[i].publishedDate).getTime();
                  for ( j = 0; j < i; j++ ){
                    comparedate = new Date(result.feed.entries[articlesort[i-j-1]].publishedDate).getTime();
                    if ( targetdate > comparedate ){
                    // 対象の記事が新しい場合、入れ替え
                      articlesort[i-j] = articlesort[i-j-1];
                      if ( i-j-1 == 0 ){
                      // 対象記事が先頭まで来た場合の処理
                        articlesort[i-j-1] = i;
                        break;
                      }
                    } else {
                    // 対象の記事が古い場合、ストップ
                      articlesort[i-j] = i;
                      break;
                    }
                  }
                }
                html += '<TABLE class="khb_ftable_ltblue" cellspacing="0">\n<TBODY>\n'
                for (var i = 0; i < result.feed.entries.length; i++) {
                    var articleno = articlesort[i];
                    var articledate = new Date(result.feed.entries[articleno].publishedDate);
                    var nowdate = new Date();
                    var keikadate = (nowdate.getTime() - articledate.getTime())/1000/3600/24;  // 経過日付
                    if ( keikadate>14 ){ break; } // 2週間を超えた分の記事は省略する
                    var dateFormat = new DateFormat("yy.MM.dd");
                    var str = dateFormat.format(articledate);
                    // 年月日入力
                    var entry = result.feed.entries[articleno];
                    if ( entry.title != "" ){  // タイトルが空白の場合、スキップする
                    if ( keikadate <= 1 ){
                      // 当日は背景色を黄色にする
                      html += '<TR>\n<TH class="date bg_bbyellow">' + str + '</TH>\n';
                    } else {
                      html += '<TR>\n<TH class="date">' + str + '</TH>\n';
                    }
                    
                    // 記事のタイトル
                    if ( keikadate <= 7 ){
                      // 1週間は「New」アイコンを表示させる
                      if ( keikadate <= 1 ){
                        // 当日は背景色を黄色にする
                        html += '<TD class="title bg_ccyellow"><a href="' + entry.link + '" target="_blank">' + entry.title + '</a><IMG src="image/bkpk1.gif"></TD>\n</TR>\n';
                      } else {
                        html += '<TD class="title"><a href="' + entry.link + '" target="_blank">' + entry.title + '</a><IMG src="image/bkpk1.gif"></TD>\n</TR>\n';
                      }
                      
                     } else {
                      html += '<TD class="title"><a href="' + entry.link + '" target="_blank">' + entry.title + '</a></TD>\n</TR>\n';
                    }
                    }
                }
           }
           html += '</TBODY></TABLE>';
       }
       // 読み込みエラー時の処理
       else {
           html = '<p>フィードの読み込みに失敗しました。</p>';
       }
       // 読み込み結果を、idが「feed」の要素に流し込む
       var container = document.getElementById("feed");
       container.innerHTML = html;
    });
}
