InfbukiData = new Array();
SYOKUNUM = 24; // 職の数

//  インフィニティ武器情報を読み込む。
//  id：職
function loadInfinityWeapon(id) {

  // サイトのタイトル
    html = '<h3>' + InfbukiData[id][0][1] + '</h3>';
  // サブメニュー
    html += '<ul class="dropdown bg_red fs_14 subwidth_180 padding_small">';
    html += '<li><A href="#top">TOP</A></li>'
    html += '<li><A href="#infiniweapondata" onclick="loadInfinityWeapon(1)">剣士</A></li>';
    html += '<li><A href="#infiniweapondata" onclick="loadInfinityWeapon(2)">戦士</A></li>';
    html += '<li><A href="#infiniweapondata" onclick="loadInfinityWeapon(3)">アチャ</A></li>';
    html += '<li><A href="#infiniweapondata" onclick="loadInfinityWeapon(4)">ランサ</A></li>';
    html += '<li><A href="#infiniweapondata" onclick="loadInfinityWeapon(5)">WIZ</A></li>';
    html += '<li><A href="#infiniweapondata" onclick="loadInfinityWeapon(6)">ワンコ</A></li>';
    html += '<li><A href="#infiniweapondata" onclick="loadInfinityWeapon(7)">BIS</A></li>';
    html += '<li><A href="#infiniweapondata" onclick="loadInfinityWeapon(8)">天使</A></li>';
    html += '<li><A href="#infiniweapondata" onclick="loadInfinityWeapon(9)">テイマ/サマナ</A></li>';
    html += '<li><A href="#infiniweapondata" onclick="loadInfinityWeapon(10)">シフ</A></li>';
    html += '<li><A href="#infiniweapondata" onclick="loadInfinityWeapon(11)">武道</A></li>';
    html += '<li><A href="#infiniweapondata" onclick="loadInfinityWeapon(12)">プリ</A></li>';
    html += '<li><A href="#infiniweapondata" onclick="loadInfinityWeapon(13)">リトル</A></li>';
    html += '<li><A href="#infiniweapondata" onclick="loadInfinityWeapon(14)">ネクロ</A></li>';
    html += '<li><A href="#infiniweapondata" onclick="loadInfinityWeapon(15)">悪魔</A></li>';
    html += '<li><A href="#infiniweapondata" onclick="loadInfinityWeapon(16)">霊術</A></li>';
    html += '<li><A href="#infiniweapondata" onclick="loadInfinityWeapon(17)">闘士</A></li>';
    html += '<li><A href="#infiniweapondata" onclick="loadInfinityWeapon(18)">光奏師魔法</A></li>';
    html += '<li><A href="#infiniweapondata" onclick="loadInfinityWeapon(19)">光奏師物理</A></li>';
    html += '<li><A href="#infiniweapondata" onclick="loadInfinityWeapon(20)">獣人</A></li>';
    html += '<li><A href="#infiniweapondata" onclick="loadInfinityWeapon(21)">メイド</A></li>';
    html += '<li><A href="#infiniweapondata" onclick="loadInfinityWeapon(22)">黒魔</A></li>';
    html += '<li><A href="#infiniweapondata" onclick="loadInfinityWeapon(23)">マスケ</A></li>';
    html += '<li><A href="#infiniweapondata" onclick="loadInfinityWeapon(24)">アルケミ</A></li>';
    html += '</ul><BR><BR><BR>';
  // １行目（武器速度、射程）
    html += '<TABLE class="mq_t ob_blue" cellspacing="0">';
    html += '<TBODY><TR class="ib_blue">';
    html += '<TD class="bs_title bg_gray">武器速度</TD><TD class="bukisokudo bg_ltblue">' + InfbukiData[id][0][2] + '秒</TD>';  // 武器速度
    html += '<TD class="syatei_title bg_gray">射程</TD><TD class="syatei bg_ltblue">' + InfbukiData[id][0][3] + '</TD>'; // 射程
    html += '</TR></TBODY></TABLE>';
  // ２行目（データヘッダ）
    html += '<TABLE class="mq_t ob_blue" cellspacing="0"><THEAD><TR class="bg_gray ib_blue bold">';
    html += '<TD class="grade2">等級</TD><TD class="ico2">　</TD><TD class="soubilv">装備Lv</TD><TD class="youkyu">要求</TD><TD class="basedmg">基礎攻撃力</TD>';
    for ( i=1; i<=InfbukiData[id][0][4]; i++ ){
      html += '<TD class="op' + i + '">OP' + i + '</TD>';
    }
    html += '</TR></THEAD>';
  // ３行目以降（データ部分）
    html += '<TBODY>';
    for ( i=1; i<=10; i++ ){
      if ( i % 2 ){
      // 奇数行
        html += '<TR class="ib_blue">';
        // 等級
        if ( InfbukiData[id][i][2] == "DX" ){
	        html += '<TH class="grade2 bg_red">' + InfbukiData[id][i][1] + "<BR>（DX）" + '</TH>';  // DXの場合
        } else {
	        html += '<TH class="grade2 bg_red">' + InfbukiData[id][i][1] + '</TH>';  // Nの場合
        }
        
        // 武器の画像
        html += '<TD class="ico2 bg_ltgray"><IMG src="' + InfbukiData[id][i][3] + '"></TD>';
        
        // 装備Lv
        html += '<TD class="soubilv bg_ltgray">' + InfbukiData[id][i][4] + '</TD>';
        
        // ステ要求
        if ( InfbukiData[id][i][5] == "-" ){
          html += '<TD class="youkyu bg_ltgray">' + InfbukiData[id][i][5] + '</TD>';  // 要求なしの場合
        } else {
          html += '<TD class="youkyu bg_ltgray">' + InfbukiData[id][i][5] + '<BR>' + InfbukiData[id][i][6] + '</TD>';  // 要求ありの場合
        }
        
        // 基礎攻撃力
        html += '<TD class="basedmg bg_ltgray">' + InfbukiData[id][i][7] + '〜' + InfbukiData[id][i][8] + '</TD>';
        
        // ＯＰデータ
        for ( j=1; j<=InfbukiData[id][0][4]; j++ ){
          if ( j <= InfbukiData[id][i][0] ){
            // データがある場合
            html += '<TD class="op' + j + ' bg_red">' + InfbukiData[id][i][j*2+7] + '<BR><FONT color="#0000ff">' + InfbukiData[id][i][j*2+8] + '</FONT></TD>';
          } else {
            // データがない場合、空白を入れる。
            html += '<TD class="op' + j + ' bg_red">　</TD>';
          }
        }
        html += '</TR>';
      } else {
      // 偶数行
        html += '<TR class="ib_blue">';
        // 等級
        if ( InfbukiData[id][i][2] == "DX" ){
	        html += '<TH class="grade2 bg_blue">' + InfbukiData[id][i][1] + "<BR>（DX）" + '</TH>';  // DXの場合
        } else {
	        html += '<TH class="grade2 bg_blue">' + InfbukiData[id][i][1] + '</TH>';  // Nの場合
        }
        
        // 武器の画像
        html += '<TD class="ico2 bg_ltgray"><IMG src="' + InfbukiData[id][i][3] + '"></TD>';
        
        // 装備Lv
        html += '<TD class="soubilv bg_ltblue">' + InfbukiData[id][i][4] + '</TD>';
        
        // ステ要求
        if ( InfbukiData[id][i][5] == "-" ){
          html += '<TD class="youkyu bg_ltblue">' + InfbukiData[id][i][5] + '</TD>';  // 要求なしの場合
        } else {
          html += '<TD class="youkyu bg_ltblue">' + InfbukiData[id][i][5] + '<BR>' + InfbukiData[id][i][6] + '</TD>';  // 要求ありの場合
        }
        
        // 基礎攻撃力
        html += '<TD class="basedmg bg_ltblue">' + InfbukiData[id][i][7] + '〜' + InfbukiData[id][i][8] + '</TD>';
        
        // ＯＰデータ
        for ( j=1; j<=InfbukiData[id][0][4]; j++ ){
          if ( j <= InfbukiData[id][i][0] ){
            // データがある場合
            html += '<TD class="op' + j + ' bg_blue">' + InfbukiData[id][i][j*2+7] + '<BR><FONT color="#0000ff">' + InfbukiData[id][i][j*2+8] + '</FONT></TD>';
          } else {
            // データがない場合、空白を入れる。
            html += '<TD class="op' + j + ' bg_blue">　</TD>';
          }
        }
        html += '</TR>';
      }
    }
    html += '</TR></TBODY></TABLE>';

    //「infweapon」のIDがついたタグに作成したhtmlコードを流し込む。
    var container = document.getElementById("infweapon");
    container.innerHTML = html;
}

